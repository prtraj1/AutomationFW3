<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Suite1" verbose="1" >
    <test name="Nopackage" >
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="testCases.TestBase" >
                <parameter name="theater" value="opera"></parameter>
                <parameter name="city" value="venice"></parameter>
                <methods>
                    <include name="TC_004"/>
                    <include name="TC_002"/>
                </methods>
            </class>
        </classes>
    </test>
    <!---->
    <test name="Nopackage 2" >
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="testCases.TestBase" ></class>
        </classes>
    </test>
    <!---->
    <test name="NoPackage 3" >
        <parameter name="browser" value="edge"></parameter>
        <groups>
            <run>
                <exclude name="regression" />
            </run>
        </groups>
        <classes>
            <class name="testCases.TestBase" ></class>
        </classes>
    </test>
</suite>
